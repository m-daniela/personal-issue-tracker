{
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "indent": ["error", 4],
    "no-multi-spaces": ["error"],
    "no-unused-vars": "off",
    "react/prop-types": "off"
},
  "ignorePatterns": [
    "node_modules",
    "build",
    "dist",
    "public"
  ]
}
